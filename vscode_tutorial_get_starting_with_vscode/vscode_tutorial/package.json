{
  "name": "vscode_tutorial",
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint:check": "eslint",
    "lint:fix": "eslint --fix",
    "format:check": "prettier --check '**/*.ts'",
    "format:fix": "prettier --write '**/*.ts'",
    "test": "jest",
    "test:create_file": "npm run test -- -t create_file",
    "test:rename_file": "npm run test -- -t rename_file",
    "test:delete_file": "npm run test -- -t delete_file",
    "test:multi_cursor": "npm run test -- -t multi_cursor",
    "test:search_and_replace": "npm run test -- -t search_and_replace/",
    "test:search_and_replace_all": "npm run test -- -t search_and_replace_all",
    "test:comment_out": "npm run test -- -t comment_out",
    "test:quick_fix": "npm run test -- -t quick_fix",
    "test:rename_symbol": "npm run test -- -t rename_symbol"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.4",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  },
  "overrides": {
    "glob": "11.0.1"
  }
}
